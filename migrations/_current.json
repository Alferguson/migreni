                    "allowNull": false,
                    "validate": {
                        "min": {
                            "args": 1,
                            "msg": "The intensity must be at least 1"
                        },
                        "max": {
                            "args": 10,
                            "msg": "The Intensity must be at most 10"
                        }
                    },
                    "seqType": "Sequelize.INTEGER"
                },
                "comment": {
                    "seqType": "Sequelize.TEXT"
                },
                "location": {
                    "seqType": "Sequelize.STRING"
                },
                "date": {
                    "defaultValue": {
