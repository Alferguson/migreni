                },
                "value": {
                    "allowNull": false,
                    "validate": {
                        "len": {
                            "args": [
                                1
                            ],
                            "msg": "Dose value cannot be empty."
                        }
                    },
                    "seqType": "Sequelize.DECIMAL"
                },
                "unit": {
                    "allowNull": false,
